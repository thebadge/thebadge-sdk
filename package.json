{
  "name": "thebadge-sdk",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "rm -rf ./dist && rollup -c --bundleConfigAsCjs",
    "lint": "eslint --max-warnings 0 --ext \".js,.ts,.tsx\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --max-warnings 0 --ext \".js,.ts,.tsx\" --ignore-path .eslintignore --fix .",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "typechain": "typechain --target=ethers-v5 ./src/contracts/abis/**/*.json --out-dir src/subgraph/generated/typechain/",
    "subgraph-codegen": "graphql-codegen",
    "test": "jest --verbose --debug",
    "test:quick": "jest --collectCoverage=false --env=jsdom --silent",
    "test:watch": "jest --collectCoverage=false --env=jsdom --watch --silent",
    "types-gen": "yarn typechain && yarn subgraph-codegen",
    "postinstall": "yarn typechain && yarn subgraph-codegen",
    "release-patch": "npm version patch && git push",
    "release-minor": "npm version minor && git push",
    "release-major": "npm version major && git push"
  },
  "dependencies": {
    "axios": "^1.2.3",
    "big-number-input": "^1.0.3",
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "remark-gfm": "^3.0.1",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "2.16.2",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "2.8.6",
    "@graphql-codegen/typescript-graphql-request": "4.5.8",
    "@graphql-codegen/typescript-operations": "2.5.11",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-image": "^3.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/dom-to-image": "^2.6.4",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "babel-jest": "^29.0.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "graphql-codegen-plugin-typescript-swr": "^0.8.3",
    "jest": "^28.0.2",
    "jest-fail-on-console": "^2.1.1",
    "jest-junit": "^13.1.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rollup": "^3.2.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.8",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4",
    "typescript-graphql-request": "^4.4.6",
    "webpack": "^4.46.0"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.8.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write",
      "bash -c tsc"
    ]
  }
}
